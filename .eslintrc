{
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "rules": {
    "strict": 0,
    "babel/object-shorthand": 1,
    "babel/generator-star": 1,
    "babel/generator-star-spacing": 0,
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
  },
  "env": {
    "browser": 1
  }
}